<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="SuiteHerokuChrome">
    <test name="HerokuChrome">
        <classes>
            <class name="theinternet.herokuapp.Tests.HerokuTestSuite">
                <parameter name="browser" value="chrome"/>
            </class>
        </classes>
    </test>

<test name="HerokuFirefox">
    <classes>
        <class name="theinternet.herokuapp.Tests.HerokuTestSuite">
            <parameter name="browser" value="firefox"/>
        </class>
    </classes>
</test>

</suite>